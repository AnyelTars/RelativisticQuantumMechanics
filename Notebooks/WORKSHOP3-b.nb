Cell[CellGroupData[{

Cell["4. Finding the roots of the transcendental Equation", "Subsection",
 CellChangeTimes->{{3.964884645607977*^9, 
  3.9648846757841597`*^9}},ExpressionUUID->"57e5f391-5321-40f3-87eb-\
58f7735206fe"],

Cell["\<\
We implement a iterative method to find the roots for a range of values of V0\
\
\>", "Text",
 CellChangeTimes->{{3.9649820159840727`*^9, 3.964982052155467*^9}, {
  3.967491500513987*^9, 
  3.967491502238554*^9}},ExpressionUUID->"eeff8b74-b69d-4321-91ba-\
5c8ad76f1ebc"],

Cell[BoxData[
 RowBox[{"\n", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"k1", "[", 
      RowBox[{"En_", "?", "NumericQ"}], "]"}], ":=", 
     RowBox[{"Sqrt", "[", 
      RowBox[{"1", "-", 
       RowBox[{"En", "^", "2"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"k2", "[", 
      RowBox[{
       RowBox[{"En_", "?", "NumericQ"}], ",", 
       RowBox[{"V0_", "?", "NumericQ"}]}], "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"s", "=", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"En", "+", "V0"}], ")"}], "^", "2"}], "-", "1"}]}], "}"}],
        ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"s", ">", "0"}], ",", 
         RowBox[{"Sqrt", "[", "s", "]"}], ",", "Indeterminate"}], "]"}]}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"even", " ", 
      RowBox[{"transcendental", ":", "k1"}]}], "=", 
     RowBox[{"k2", " ", 
      RowBox[{"Tan", "[", 
       RowBox[{"a", " ", "k2"}], "]"}]}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"fEven", "[", 
      RowBox[{
       RowBox[{"En_", "?", "NumericQ"}], ",", 
       RowBox[{"V0_", "?", "NumericQ"}]}], "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"k2v", "=", 
         RowBox[{"k2", "[", 
          RowBox[{"En", ",", "V0"}], "]"}]}], "}"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"!", 
           RowBox[{"NumericQ", "[", "k2v", "]"}]}], "||", 
          RowBox[{"!", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "<", "En", "<", "1"}], ")"}]}]}], ",", 
         "Indeterminate", ",", 
         RowBox[{
          RowBox[{"k1", "[", "En", "]"}], "-", 
          RowBox[{"k2v", " ", 
           RowBox[{"Tan", "[", 
            RowBox[{"a", " ", "k2v"}], "]"}]}]}]}], "]"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"--", 
      RowBox[{"--", 
       RowBox[{"-", "FIRST"}]}]}], " ", "even", " ", "root", " ", 
     RowBox[{"only", ":", 
      RowBox[{
       RowBox[{"enforce", " ", "0"}], "<", 
       RowBox[{"a", " ", "k2"}], "<", 
       RowBox[{
        RowBox[{"\[Pi]", "/", 
         RowBox[{
          RowBox[{"2", "--"}], "--"}]}], "-"}]}]}]}], "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"firstEvenRoot", "[", 
      RowBox[{
       RowBox[{"V0_", "?", "NumericQ"}], ",", 
       RowBox[{"n_", ":", "2000"}]}], "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"grid", ",", "vals", ",", "pairs", ",", "brackets"}], "}"}], ",", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"scan", " ", "E", " ", "from", " ", "high"}], "->", 
         RowBox[{
         "low", " ", "so", " ", "we", " ", "pick", " ", "the", " ", "ground", 
          " ", "branch", " ", "first"}]}], "*)"}], 
       RowBox[{
        RowBox[{"grid", "=", 
         RowBox[{"Subdivide", "[", 
          RowBox[{"0.999999", ",", 
           RowBox[{"-", "0.999999"}], ",", "n"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"vals", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"Module", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"E", "=", "e"}], ",", 
               RowBox[{"k2v", "=", 
                RowBox[{"k2", "[", 
                 RowBox[{"e", ",", "V0"}], "]"}]}], ",", "x"}], "}"}], ",", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"NumericQ", "[", "k2v", "]"}], ",", 
               RowBox[{
                RowBox[{"x", "=", 
                 RowBox[{"a", " ", "k2v"}]}], ";", "\[IndentingNewLine]", 
                RowBox[{"If", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"0", "<", "x", "<", 
                    RowBox[{"Pi", "/", "2"}]}], "&&", 
                   RowBox[{"NumericQ", "[", 
                    RowBox[{"Tan", "[", "x", "]"}], "]"}]}], ",", 
                  RowBox[{
                   RowBox[{"k1", "[", "E", "]"}], "-", 
                   RowBox[{"k2v", " ", 
                    RowBox[{"Tan", "[", "x", "]"}]}]}], ",", 
                  "Indeterminate"}], "]"}]}], ",", "Indeterminate"}], "]"}]}],
             "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"e", ",", "grid"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"pairs", "=", 
         RowBox[{"Partition", "[", 
          RowBox[{
           RowBox[{"Transpose", "[", 
            RowBox[{"{", 
             RowBox[{"grid", ",", "vals"}], "}"}], "]"}], ",", "2", ",", 
           "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"brackets", "=", 
         RowBox[{"Select", "[", 
          RowBox[{"pairs", ",", 
           RowBox[{
            RowBox[{
             RowBox[{"VectorQ", "[", 
              RowBox[{
               RowBox[{"#", "[", 
                RowBox[{"[", 
                 RowBox[{"All", ",", "2"}], "]"}], "]"}], ",", "NumericQ"}], 
              "]"}], "&&", 
             RowBox[{
              RowBox[{"Times", "@@", 
               RowBox[{"Sign", "[", 
                RowBox[{"#", "[", 
                 RowBox[{"[", 
                  RowBox[{"All", ",", "2"}], "]"}], "]"}], "]"}]}], "<", 
              "0"}]}], "&"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"brackets", "===", 
           RowBox[{"{", "}"}]}], ",", 
          RowBox[{"Missing", "[", "\"\<NoRoot\>\"", "]"}], ",", 
          RowBox[{"En", "/.", " ", 
           RowBox[{"FindRoot", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"fEven", "[", 
               RowBox[{"En", ",", "V0"}], "]"}], "==", "0"}], ",", 
             RowBox[{"{", 
              RowBox[{"En", ",", 
               RowBox[{"brackets", "[", 
                RowBox[{"[", 
                 RowBox[{"1", ",", "1", ",", "1"}], "]"}], "]"}], ",", 
               RowBox[{"brackets", "[", 
                RowBox[{"[", 
                 RowBox[{"1", ",", "2", ",", "1"}], "]"}], "]"}]}], "}"}], ",", 
             RowBox[{"(*", 
              RowBox[{
               RowBox[{"first", " ", "bracket"}], "->", 
               RowBox[{"first", " ", "branch"}]}], "*)"}], 
             RowBox[{"Method", "->", "\"\<Brent\>\""}], ",", 
             RowBox[{"MaxIterations", "->", "300"}]}], "]"}]}]}], "]"}]}]}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "sweep", " ", "V0", " ", "and", " ", "collect", " ", "just", " ", "that", 
     " ", "line"}], "*)"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"sweepFirstEven", "[", 
      RowBox[{
       RowBox[{"vmin_", ":", "0.10"}], ",", 
       RowBox[{"vmax_", ":", "2.05"}], ",", 
       RowBox[{"dV_", ":", "0.05"}]}], "]"}], ":=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"V0", ",", 
         RowBox[{"firstEvenRoot", "[", "V0", "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"V0", ",", 
         RowBox[{"N", "@", 
          RowBox[{"Range", "[", 
           RowBox[{"vmin", ",", "vmax", ",", "dV"}], "]"}]}]}], "}"}]}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", "\n", 
   RowBox[{
    RowBox[{"line", "=", 
     RowBox[{"sweepFirstEven", "[", 
      RowBox[{"0.10", ",", "2.05", ",", "0.05"}], "]"}]}], ";"}]}]}]], "Input",\

 CellChangeTimes->{{3.9649808330050507`*^9, 3.964980833005164*^9}, {
   3.9649808667449017`*^9, 3.964980888387247*^9}, {3.964981015647543*^9, 
   3.964981027544324*^9}, {3.964981286321445*^9, 3.9649812867256317`*^9}, {
   3.964981324728416*^9, 3.96498132533539*^9}, {3.9649813671058693`*^9, 
